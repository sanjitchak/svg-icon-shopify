 <div class="grid__item medium-up--one-whole footer_credits">
        <p class="credits">
          &copy; {{ "now" | date: "%Y" }} 
          {% if section.settings.shop_name != blank %}
          <a href="/">{{ section.settings.shop_name }}</a>
          {% else %}
           {{ shop.name | link_to: '/' }}
          {% endif %}

        
          
          {% if section.settings.display_shopify_by %} &
          <a href="https://www.shopify.in/?ref=limitless-theme&utm_campaign=footer-link" target="_blank">Powered by shopify</a>.
          {% endif %}
        </p>

        {% if section.settings.display_payment_methods %}
        <div class="site-footer__payment-icons site-footer__payment-icons--right">
